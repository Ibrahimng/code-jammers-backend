{ 
  "root": true,
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es2021": true,
    "mocha": true
  },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": ["error", "windows"],
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "no-unused-vars":"off",
      "no-undef":"off",
      "import/no-dynamic-require":"off",
      "no-tabs":"off",
      "global-require":"off",
      "no-console": "off",
      "import/no-extraneous-dependencies":["error", {"devDependencies": true}],
      "no-useless-catch":"off",
      "import/no-dynamic-require":"off"
    }
  }